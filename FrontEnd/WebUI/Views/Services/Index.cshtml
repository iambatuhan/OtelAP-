@using WebUI.Dtos.ServicesDto
@model List<ResultServiceDto>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Hizmet Başlagı </th>
        <th>Sil </th>
        <th>Güncelle </th>

    </tr>
    @foreach(var item in Model)
            {
                <tr>
                    <td>@item.ServicesRoom</td>
                    <td>@item.Title</td>
                    <td><a href="/Services/DeleteServices/@item.ServicesRoom" class="btn btn-danger">Sil</a></td>
                    <td><a href="/Services/UpdateServices/@item.ServicesRoom" class="btn btn-danger">Güncelle</a></td>



                </tr>
            }
</table>
<br />

<a href="/Services/AddService" class="btn btn-danger"></a>

